def reverse_complement(seq):
    rna = ''
    for symbol in seq:
        if symbol == 'A':
            rna = rna + 'T'
        elif symbol == 'T':
            rna = rna + 'A'
        elif symbol == 'C':
            rna = rna + 'G'
        elif symbol == 'G':
            rna = rna + 'C'
    return (rna[::-1])
